    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^2.7.0",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.0",
    "webpack-dev-server": "^3.3.1"
